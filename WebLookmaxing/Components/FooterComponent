                    if (MediaQuery.sizeOf(context).width < kBreakpointSmall) {
                      return false;
                    } else if (MediaQuery.sizeOf(context).width <
                        kBreakpointMedium) {
                      return true;
                    } else if (MediaQuery.sizeOf(context).width <
